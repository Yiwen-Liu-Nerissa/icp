import java.util.Scanner;

class Main {
  public static void main(String[] args) {
    Scanner scannerInput = new Scanner(System.in);
  
   //additional personal information of John Doe

   System.out.println("What is the eye color of John Doe?");

   String eyeColor = scannerInput.nextLine();
   
   System.out.println("John Doe's eye color is " + eyeColor);

  }
}
